{
	"port": 8898,
	"host": "127.0.0.1",
	"flushInterval": 1000,
	"osStats": "all", 
	"backends": {
                "stdout": {
                        "path": "./backends/stdout",
                        "date_function": "toLocaleDateString"
                },

"elasticsearch": {
	"path": "./backends/elasticsearch",
        "server": {
          "host": "localhost",
          "port": 9200
        },
        "timeOptions": {
          "_index": "stats",
          "_type": "time"
        },
        "countOptions": {
          "_index": "stats",
          "_type": "count"
        },
        "osOptions"
        : {
          "_index": "stats",
          "_type": "os"
        }
      }

	}
}
